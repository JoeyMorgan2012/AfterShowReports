  <DataSets>
    <DataSet Name="AprilShowBySales">
      <Query>
        <DataSourceName>ELK_DM</DataSourceName>
        <CommandText>SELECT        Brand, ItemCode, ItemDescription, QtySold, Sales, Price, ImageUrl
FROM            AprilBySales 
ORDER BY Sales DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Brand">
          <DataField>Brand</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemCode">
          <DataField>ItemCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemDescription">
          <DataField>ItemDescription</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="QtySold">
          <DataField>QtySold</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Price">
          <DataField>Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ImageUrl">
          <DataField>ImageUrl</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Brands">
      <Query>
        <DataSourceName>ELK_DM</DataSourceName>
        <CommandText>WITH brands AS (SELECT        Brand
                                      FROM            AprilByQty AS abq
                                      UNION
                                      SELECT        Brand
                                      FROM            AprilBySales AS abs)
    SELECT DISTINCT Brand
     FROM            brands AS brands_1
     ORDER BY Brand</CommandText>
      </Query>
      <Fields>
        <Field Name="Brand">
          <DataField>Brand</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
